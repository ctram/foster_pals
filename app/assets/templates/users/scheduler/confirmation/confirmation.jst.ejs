<% for (var i = 0; i < stays.models.length; i++) { %>
  <% var animal = stays.models[i] %>
  <div>
    <img src=<%= animal.escape('main_image_thumb_url') %>>
    <span class='strong'>
      <%= animal.escape('name') %>
    </span>
  </div>
<% } %>


