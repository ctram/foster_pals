<h3>Requests Sent for the Following!</h3>

<% for (var i = 0; i < stays.models.length; i++) { %>
  <% var animal = stays.models[i] %>
  <div>
    <img src=<%= animal.escape('main_image_thumb_url') %>>
    <span class='strong'>
      <%= animal.escape('name') %>
    </span>
    
    <span class='strong'>
      <%= animal.escape('status') %>
    </span>
  </div>
<% } %>
