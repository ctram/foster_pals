<% if (CURRENT_USER_ID !== parseInt(user.escape('id'))) { %>
  <button class='btn btn-primary schedule-btn' data-user-id=<%= user.escape( 'id')%> > 
    Click to Request a Reservation for your Pet!
  </button>
<% } else { %>
  <button class='btn btn-primary edit-profile-btn'>
    Edit Profile
  </button>
<% } %>

<script>
  setTimeout(function () {
    $('.highlight-request-btn').removeClass('slideInRight').addClass('fadeOut');
  }, 3000)
</script>