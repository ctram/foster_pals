<link rel="stylesheet" href="//frontend.reklamor.com/fancybox/jquery.fancybox.css" media="screen">
<script src="//frontend.reklamor.com/fancybox/jquery.fancybox.js"></script>

<div class="container">
	<div class="row">
		<div class='list-group gallery'>
      <% for (var i = 0; i < images.models.length; i++) { %>
        <% var image = images.models[i] %>
        <div class='col-sm-4 col-xs-6 col-md-3 col-lg-3 img-thumb'>
            <a class="thumbnail fancybox" rel="lightbox" href=<%= image.escape('url') %> >
              <img class="img-responsive" alt="" src=<%= image.escape('thumb_url') %> />
            </a>
        </div> <!-- col-6 / end -->
      <% } %>
    </div> <!-- list-group / end -->
	</div> <!-- row / end -->
</div> <!-- container / end -->

<script>
  $(document).ready(function(){
      //FANCYBOX
      //https://github.com/fancyapps/fancyBox
      $(".fancybox").fancybox({
          openEffect: "none",
          closeEffect: "none"
      });
  });
</script>
